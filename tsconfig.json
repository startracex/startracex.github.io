{
  "extends": "@startracex/dev-config/tsconfig/app",
  "compilerOptions": {
    "jsx": "preserve",
    "incremental": true,
    "strict": false,
    "noEmit": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/.source": ["./.source/index.ts"],
      "@/*": ["./src/*"]
    }
  },
  "include": ["*.ts", "src/**/*.ts", "src/**/*.tsx", ".next/types/**/*.ts"],
  "exclude": []
}
